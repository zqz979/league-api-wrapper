{
  "name": "@zqz979/league-api-wrapper",
  "version": "0.0.4",
  "description": "TypeScript wrapper for Riot Games League of Legends API",
  "keywords": [
    "riot",
    "league-of-legends",
    "api",
    "typescript",
    "gaming",
    "lol"
  ],
  "author": "Qunzhe Zhu",
  "license": "MIT",
  "homepage": "https://github.com/zqz979/league-api-wrapper",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zqz979/league-api-wrapper.git"
  },
  "bugs": {
    "url": "https://github.com/zqz979/league-api-wrapper/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf node_modules dist",
    "check:types": "tsc --noEmit",
    "check:format": "prettier --check .",
    "check:lint": "eslint . --ext .ts,.js",
    "watch": "tsc --watch",
    "build": "tsc",
    "build:clean": "rm -rf dist && pnpm build",
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build:clean && pnpm typecheck && pnpm lint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/clients/types.js",
      "types": "./dist/clients/types.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "prettier": "3.6.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.39.1"
  },
  "dependencies": {
    "axios": "^1.11.0"
  }
}
